; load special address into %ix
  MOVL %ix, $69
  MOVH %ix, $69

  
  MOVL %ax, $45
  MOVL %bx, $20
  MOVL %cx, #4
  MOVL %dx, $0F
  STORE %ax, [%ix]
  ADD %ax, %bx
  STORE %ax, [%ix]
  AND %ax, %dx
  STORE %ax, [%ix]
  OR %bx, %dx
  STORE %bx, [%ix]
  LSL %bx, %cx
  STORE %bx, [%ix]
  LSR %bx, %cx
  STORE %bx, [%ix]
  HALT
